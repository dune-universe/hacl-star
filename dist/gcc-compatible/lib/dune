(install
 (files ../config.h)
 (section lib)
 (package hacl-star-raw))

(include dune.inc)

(rule
 (deps
  (:c_deps ../ctypes.depend)
  (source_tree .))
 (action
  (with-stdout-to
   dune.inc.gen
   (run ../gen_dune_rules.exe --dir lib %{c_deps}))))

(rule
 (alias runtest)
 (action
  (diff dune.inc dune.inc.gen)))
